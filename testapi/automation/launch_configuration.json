{
    "name": "{{PROJECTNAME}} Attach",
    "type": "coreclr",
    "request": "attach",
    "processId": "${command:pickRemoteProcess}",
    "pipeTransport": {
        "pipeProgram": "ssh",
        "pipeArgs": ["-T", "root@{{PROJECTIP}}"],
        "debuggerPath": "/app/vsdbg/vsdbg",
        "pipeCwd": "${workspaceRoot}",
        "quoteArgs": true
    },
    "sourceFileMap": {
        "/app/api/{{PROJECTNAME}}": "${workspaceRoot}"
    }
}